{
  "name": "qios-retail-system",
  "version": "1.0.0",
  "description": "Quality Integrated Omni System - Comprehensive retail management with payment gateway integration",
  "main": "electron/main.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "next build && electron-builder",
    "electron:build:win": "next build && electron-builder --win",
    "electron:build:mac": "next build && electron-builder --mac",
    "electron:build:linux": "next build && electron-builder --linux"
  },
  "build": {
    "appId": "com.qios.retail",
    "productName": "QIOS Retail System",
    "directories": {
      "output": "dist"
    },
    "files": [
      "electron/**/*",
      ".next/**/*",
      "public/**/*",
      "package.json"
    ],
    "npmRebuild": false,
    "asarUnpack": [
      "node_modules/bcrypt/**/*",
      "node_modules/@prisma/**/*"
    ],
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "public/icon.ico"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "public/icon.icns",
      "category": "public.app-category.business"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "public/icon.png",
      "category": "Office"
    }
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@hookform/resolvers": "^3.3.0",
    "@prisma/client": "^5.22.0",
    "@types/node": "^20.19.24",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.2.0",
    "axios": "^1.6.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.3",
    "electron-next": "^3.1.5",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "framer-motion": "^10.16.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.292.0",
    "midtrans-client": "^1.3.0",
    "next": "^14.0.0",
    "next-themes": "^0.4.6",
    "openai": "^6.7.0",
    "pg": "^8.16.3",
    "prisma": "^5.22.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.0",
    "recharts": "^2.8.0",
    "typescript": "^5.9.3",
    "validator": "^13.15.20",
    "xendit-node": "^1.0.0",
    "zod": "^3.25.76",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.0",
    "@types/jsonwebtoken": "^9.0.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.2",
    "electron": "^39.2.5",
    "electron-builder": "^25.1.8",
    "eslint": "^8.53.0",
    "eslint-config-next": "^14.0.0",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "wait-on": "^8.0.5"
  },
  "keywords": [
    "retail",
    "payment-gateway",
    "umkm",
    "pos",
    "analytics",
    "ai",
    "saas"
  ],
  "author": "QIOS Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theoneandonlyvabo/QIOS.git"
  },
  "bugs": {
    "url": "https://github.com/theoneandonlyvabo/QIOS/issues"
  },
  "homepage": "https://github.com/theoneandonlyvabo/QIOS#readme"
}